<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
 
    <Appenders>
        <File name="file" FileName="log.log">
            <PatternLayout pattern="%logger{36}%msg%n"/>
        </File>
    </Appenders>
 
    <Loggers>
        <Root level="info">
            <AppenderRef ref="file"/>
        </Root>
    </Loggers>
</Configuration>